    "@babel/register": "^7.11.5",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "jest": "^26.4.2",
